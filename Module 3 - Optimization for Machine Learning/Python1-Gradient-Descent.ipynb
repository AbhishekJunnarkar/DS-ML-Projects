    "# simple gradient descent for f(x) = x^2\n",
    "x = 10\n",
    "learning_rate = 0.1\n",
    "\n",
    "for _ in range(20):\n",
    "    grad = 2 * x   # derivative of x^2\n",
    "    x = x - learning_rate * grad\n",
    "    print(x)\n"
   ]
